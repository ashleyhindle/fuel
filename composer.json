{
    "name": "laravel-zero/laravel-zero",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.17",
        "laravel-zero/framework": "^12.0.2",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "icanhazstring/composer-unused": "^0.9.5",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpacker/phpacker": "*",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest --parallel --compact",
        "test:coverage": "XDEBUG_MODE=coverage php -d memory_limit=-1 vendor/bin/pest --parallel --coverage --min=60 --compact",
        "test:type-coverage": "php -d memory_limit=-1 vendor/bin/pest --type-coverage --min=100 --compact",
        "format": "pint",
        "rector": "rector process",
        "rector:check": "rector process --dry-run",
        "fix": ["@format", "@rector"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["fuel"]
}
